package com.javalec.ex;

public class HanwhaCompany {
	
	private String companyName;
	private String companySector;
	private String groupPresident;
	private int companyEmployeeCount;
	
	public String getCompanyName() {
		return companyName;
	}
	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}
	public String getCompanySector() {
		return companySector;
	}
	public void setCompanySector(String companySector) {
		this.companySector = companySector;
	}
	public String getGroupPresident() {
		return groupPresident;
	}
	public void setGroupPresident(String groupPresident) {
		this.groupPresident = groupPresident;
	}
	public int getCompanyEmployeeCount() {
		return companyEmployeeCount;
	}
	public void setCompanyEmployeeCount(int companyEmployeeCount) {
		this.companyEmployeeCount = companyEmployeeCount;
	}
	@Override
	public String toString() {
		return "HanwhaCompany [companyName=" + companyName + ", companySector=" + companySector + ", groupPresident="
				+ groupPresident + ", companyEmployeeCount=" + companyEmployeeCount + "]";
	}
	
}
