//2019.8.19 Java Basic 실습
package test;

public class DataTypeTest {
	public static void main(String[] args) {
//		1. 기본 자료형 8개 변수 선언해서 출력하기
		boolean a = true;
		char b = 'b';
		byte c = 1;
		short d = 2;
		int e = 3;
		long f = 4;
		float g = 5;
		double h = 6;
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);
		System.out.println(g);
		System.out.println(h);
		System.out.println();
		
//		2. 8개 래퍼 클래스 객체 생성해서 출력
		Boolean a1 = new Boolean(a);
		Character a2 = new Character(b);
		Byte a3 = new Byte(c);
		Short a4 = new Short(d);
		Integer a5 = new Integer(e);
		Long a6 = new Long(f);
		Float a7 = new Float(g);
		Double a8 = new Double(h);

		System.out.println(a1);
		System.out.println(a2);
		System.out.println(a3);
		System.out.println(a4);
		System.out.println(a5);
		System.out.println(a6);
		System.out.println(a7);
		System.out.println(a8);
		
//		3. 범위를 넘어가는 값을 선언하면 어떤 일이 벌어지는지
		int test = Integer.MAX_VALUE;
		System.out.println(++test);
		
	}
}
